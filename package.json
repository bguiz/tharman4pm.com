{
  "name": "tharman4pm.com",
  "version": "0.0.0",
  "description": "Tharman for PM",
  "main": "index.js",
  "scripts": {
    "downloadssg": "wget https://github.com/spf13/hugo/releases/download/v0.14/hugo_0.14_linux_amd64.tar.gz",
    "installssg": "tar -xvf hugo_0.14_linux_amd64.tar.gz && cd hugo_0.14_linux_amd64 && ln -s ./hugo_0.14_linux_amd64 hugo && hugo version",
    "installtheme": "mkdir -p ./src/themes && cd ./src/themes && git clone https://github.com/vjeantet/hugo-theme-casper.git casper",
    "generatedocs": "export PATH=${PWD}/hugo_0.14_linux_amd64:${PATH} && cd ./src && hugo version && hugo",
    "autodocs": "node ./node_modules/autodocs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bguiz/tharman4pm.com.git"
  },
  "keywords": [
    "tharman",
    "pm"
  ],
  "author": "bguiz",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/bguiz/tharman4pm.com/issues"
  },
  "homepage": "https://github.com/bguiz/tharman4pm.com#readme",
  "devDependencies": {
    "autodocs": "^0.7.1"
  }
}
